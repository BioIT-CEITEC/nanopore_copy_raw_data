{
    "workflow_description": {
      "name":"nanopore_copy_raw_data",
      "version": 1.0,
      "label":"Copy raw signals",
      "type":"run_demultiplexing",
      "inputs":"{run_dir}/*",
      "outputs":[
        "nanopore_run_report.json",
        "raw_*5_files"
      ],
      "run_parameters": {
        "cpu": 1,
        "memory": 4
      },
      "run_outputs":[
        "{library}/sequencing_run_info/*"
      ]
    },
    "general_params": [
      "run_dir"
    ]
  }
  